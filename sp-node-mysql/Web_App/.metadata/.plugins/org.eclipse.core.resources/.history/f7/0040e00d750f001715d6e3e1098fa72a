
var usersApp = angular.module('usersApp', []);
usersApp.controller('UsersListCrtl', function($scope, $http) {
	$http.get('http://192.168.1.6:3000/users').success(function(data) {
		$http.get('http://192.168.1.6:3000/user_info/' + data.user_id).success(function(fullData) {
			$scope.userData = fullData;
		});
	});
});